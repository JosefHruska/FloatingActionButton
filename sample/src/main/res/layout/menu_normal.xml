<?xml version="1.0" encoding="utf-8"?>
<com.github.clans.fab.FloatingActionMenu android:id="@+id/fab"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="bottom|right"
    android:paddingBottom="20dp"
    android:paddingRight="24dp"
    android:src="@drawable/ic_drawing"
    app:menu_backgroundColor="@color/black_a160"
    app:menu_fab_label="MENU 1"
    app:menu_extendedButtonText="Add new expense"
    app:menu_isExtended="false"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/menu_normal_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:src="@drawable/ic_star"
        app:fab_label="EXPENSE 1"
        app:fab_size="normal" />

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/menu_normal_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:src="@drawable/ic_star"
        app:fab_label="TRANSFER 1"
        app:fab_size="normal" />
</com.github.clans.fab.FloatingActionMenu>